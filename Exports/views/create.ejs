      <h2>Events</h2>
      <input type="text" name="eventsType" placeholder="Type">
      <input type="text" name="events">

      <h2>Goals</h2>
      <input type="text" name="goalsType" placeholder="Type">
      <input type="text" name="goals">

      <h2>Included documentation</h2>
      <input type="text" name="includedType" placeholder="Type">
      <input type="text" name="included">

      <h2>Required documentation</h2>
      <input type="text" name="requiredType" placeholder="Type">
      <input type="text" name="required">
      <br><br>

      <input type="submit">
    </form>
